{
    "name": "espn-fantasy-autopick",
    "version": "2.4.5",
    "description": "A Chrome extension that lets you automatically add active players to the current roster in an ESPN NBA fantasy league.",
    "repository": "https://github.com/magnusbakken/espn-fantasy-autopick.git",
    "author": "Magnus Grindal Bakken <magnusbakken@gmail.com>",
    "license": "MIT",
    "scripts": {
        "package": "node scripts/zip.js --inputFiles=input-files.json --zipPath=build/espn-fantasy-autopick.zip",
        "bump-version": "node scripts/version.js --manifestFilePath=manifest.json --packageFilePath=package.json"
    },
    "devDependencies": {
        "node-zip": "^1.1.1",
        "command-line-args": "^5.0.1"
    }
}